# dashboard.yml.j2
apiVersion: 1

providers:
  - name: 'default'
    orgId: 1
    folder: '{{ grafana_dashboard_folder_name }}'
    type: file
    disableDeletion: false
    editable: true
    updateIntervalSeconds: 10
    allowUiUpdates: false
    options:
      path: /var/lib/grafana/dashboards/{{ grafana_dashboard_subfolder }}
